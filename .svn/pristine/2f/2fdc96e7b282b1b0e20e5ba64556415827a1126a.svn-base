/*==================================
Table (Vertical, Colr) : tbl_vert_color
==================================*/
/* Basic */
.tbl_vert_color{border-top:1px solid #4b4750;}
.tbl_vert_color_inside {width:100%; text-align:center; color:#333333; background-color:#fff; }
.tbl_vert_color_inside tbody {border:1px solid #ccd2de;}
.tbl_vert_color_inside .vert_t_data {padding:6px 10px; border: 1px solid #ccd2de; text-align:left;}
.tbl_vert_color_inside thead .vert_t_head { font-weight:400; padding:6px 10px; border: 1px solid #ccd2de; border-bottom-color: #aaa; border-top:none; background-color:#eef3fd;}
.tbl_vert_color_inside thead .vert_t_head .array_section {position:relative;}
.tbl_vert_color_inside thead .vert_t_head .tit{display:inline-block; margin-right:10px;}
.tbl_vert_color_inside .vert_t_head .array {color:#333333; text-align:center;}
.tbl_vert_color_inside .vert_t_head .vert_t_ico {display: inline-block; position:absolute; top:50%; right:0px; margin-top:-3px; vertical-align: middle; background: url("../images/table01/tbl_vert_color.png"); }
.tbl_vert_color_inside .vert_t_head .vert_t_ico.ico_descending {display: inline-block; background-position: left top; width: 7px; height: 4px;}
.tbl_vert_color_inside .vert_t_head .vert_t_ico.ico_ascending {display: inline-block; background-position: 30px top; width: 7px; height: 4px;}

/* Sum */
.tbl_vert_color.sum tbody .vert_t_head{font-weight:normal;}
.tbl_vert_color.sum tbody .vert_t_head.align_left{padding-left:10px ;padding-right:10px;}

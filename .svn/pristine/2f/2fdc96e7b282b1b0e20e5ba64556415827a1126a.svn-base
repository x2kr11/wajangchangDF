/*==================================
Box Table 
==================================*/
.tbl_box {}
.tbl_box .tbl_box_list {position:relative; padding:15px 10px; border:1px solid #e0e0e0; border-left-width:5px; margin-bottom:10px;}
.tbl_box .tbl_box_list.cate01 {border-left-color:#fb6c07;}
.tbl_box .tbl_box_list.cate02 {border-left-color:#4bbeb7;}
.tbl_box .tbl_box_list.cate03 {border-left-color:#55b2d1;}
.tbl_box .tbl_box_wrap {position:relative; display:table; width:100%; }
.tbl_box .tbl_box_left {display:table-cell; width:50%; vertical-align:top;}
.tbl_box .tbl_box_right {display:table-cell; vertical-align:top;}

/* left */
.tbl_box .jqTransformCheckboxWrapper {position:absolute; left:0; top:0;}
.tbl_box .info_area {margin-left:25px;}
.tbl_box .info_area .tit {margin-bottom:3px;}
.tbl_box .info_area .tit_link {font-size:14px; line-height:1.4em; color:#333; font-weight:bold;}
.tbl_box .info_area .tit_sub {font-size:12px; line-height:1.4em; color:#84868c; font-weight:bold; margin-bottom:8px;}
.tbl_box .info_area .desc {font-size:12px; line-height:1.4em; color:#84868c; }

/* right */
.tbl_box .tbl_inner {width:100%;}
.tbl_box .inner_head {font-size:12px; line-height:1.33em; color:#96989c; font-weight:normal; text-align:right; padding:2px 0 2px 30px;}
.tbl_box .inner_data {font-size:12px; line-height:1.33em; color:#595c65; padding:2px 0 2px 15px;}
.tbl_box .btn_option {position:absolute; right:0; top:0; width:24px; height:24px; cursor:pointer; background-color:#f2f2f2;}
.tbl_box .btn_option .tbl_box_ico_option {margin-top:5px; margin-left:5px;}

/* option box */
.tbl_box .tbl_box_option {position:absolute; display:none; right:0; top:0; height:100%; background-color:rgba(25, 39, 39, .85); }
.tbl_box .tbl_box_option .opt_list {padding:10px 40px 10px 15px;}
.tbl_box .tbl_box_option .opt_item {margin:2px 0;}
.tbl_box .tbl_box_option .opt_link {cursor:pointer;}
.tbl_box .tbl_box_option .opt_link .txt {font-size:12px; line-height:1.4em; color:#adbed9; vertical-align:middle;}
.tbl_box .tbl_box_option .opt_link:hover .txt {color:#fff;}
.tbl_box .tbl_box_option .opt_link:hover .tbl_box_ico {background-position-y:-50px;}
.tbl_box .tbl_box_option .btn_option_close {position:absolute; right:5px; top:5px; cursor:pointer;}
.tbl_box .tbl_box_option .btn_option_close:hover .tbl_box_ico {background-position:left -50px;}

/* icon */
.tbl_box_ico {display:inline-block; vertical-align:middle; background:url(../images/table01/tbl_box.png) no-repeat;}
.tbl_box_ico_option {width:14px; height:14px; background-position:left top;}
.tbl_box_ico_close {width:20px; height:20px; background-position:left -25px;}
.tbl_box_ico_01 {width:20px; height:20px; background-position:-25px -25px;}
.tbl_box_ico_02 {width:20px; height:20px; background-position:-50px -25px;}
.tbl_box_ico_03 {width:20px; height:20px; background-position:-75px -25px;}
.tbl_box_ico_04 {width:20px; height:20px; background-position:-100px -25px;}

.schedule{border-top:1px solid #757984; margin-top:10px;}

/* Basic */
.schedule_inside {width:100%; color:#333333; background-color:#fff; table-layout:fixed; border-collapse:collapse;}
.schedule_inside thead .schedule_t_head {border:1px solid #d2d4d9; border-bottom-color:#d2d4d9; border-top:none;}
.schedule_inside tbody .schedule_t_head, .tbl_schedulet tfoot .schedule_t_head {border:1px solid #d2d4d9;} 
.schedule_inside thead .schedule_t_head {font-weight:normal; padding:6px 10px; border-right:1px solid #d2d4d9; background-color:#e7e7e7; text-align:center; color:#000676767}
.schedule_inside .schedule_t_data {padding:6px 10px; border:1px solid #ccd2de; border-top-width:0; position:relative;}
.schedule_inside tbody {border-top:1px solid #aaa;}


.tbl_hori input, .tbl_vert input, .filebox label, board_liet input{margin-top:-2px!important;}
.tbl_vert input[type="text"] +  .btn_txt{margin-top:-2px!important;}
.tbl_hori input[type="text"] +  .btn_txt{margin-top:-2px!important;}
.board_list input[type="text"] +  .btn_txt{margin-top:-2px!important;}

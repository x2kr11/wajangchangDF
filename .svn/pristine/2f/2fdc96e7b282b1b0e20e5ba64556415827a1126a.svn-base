.tbl_vert_color.sum .vert_t_row.sum .vert_t_head,
.tbl_vert_color.sum .vert_t_row.sum {font-weight:bold; }
.tbl_vert_color.sum .vert_t_row .vert_t_head {border-top:none; border:1px solid #ccd2de;}
.tbl_vert_color.sum thead tr:first-child .vert_t_head{border-right:1px solid #ccd2de; border-bottom:1px solid #aaa; border-top:none;}

/* combine */
.tbl_vert_color_inside.combine .vert_t_row.combine .vert_t_data,
.tbl_vert_color_inside.combine .vert_t_row .vert_t_data.combine {border-bottom: 1px solid #ccd2de;}

/* BG Color */
.tbl_vert_color_inside .bg_1, .tbl_hori_color_inside .bg_1 {background-color:#fef3e2!important;} /* orange */
.tbl_vert_color_inside .bg_2, .tbl_hori_color_inside .bg_2 {background-color:#e5f3f8!important;} /* blue */
.tbl_vert_color_inside .bg_3, .tbl_hori_color_inside .bg_3 {background-color:#ffeeef!important;} /* pink */
.tbl_vert_color_inside .bg_4, .tbl_hori_color_inside .bg_4 {background-color:#f4f6e3!important;} /* green */
.tbl_vert_color_inside .bg_5, .tbl_hori_color_inside .bg_5 {background-color:#edeef3!important;} /* grey */
.tbl_vert_color_inside .bg_6, .tbl_hori_color_inside .bg_6 {background-color:#e7e7e7!important;} /* grey */
.tbl_vert_color_inside .bg_7, .tbl_hori_color_inside .bg_7 {background-color:#f5eefa!important;} /* purple */

/* Table Sum BG Color */
.tbl_vert_color_inside .bg_sum_1, .tbl_hori_color_inside .bg_sum_1 {background-color:#eee6dd!important;} /* orange + grey */
.tbl_vert_color_inside .bg_sum_2, .tbl_hori_color_inside .bg_sum_2 {background-color:#d9e6f0!important;} /* blue + grey */
.tbl_vert_color_inside .bg_sum_3, .tbl_hori_color_inside .bg_sum_3 {background-color:#efe2e8!important;} /* pink + grey */
.tbl_vert_color_inside .bg_sum_4, .tbl_hori_color_inside .bg_sum_4 {background-color:#e6e9de!important;} /* green + grey */
.tbl_vert_color_inside .bg_sum_5, .tbl_hori_color_inside .bg_sum_5 {background-color:#eff0f6!important;} /* white + grey */
.tbl_vert_color_inside .bg_sum_6, .tbl_hori_color_inside .bg_sum_6{background-color:#e2e4ee!important;} /* dark grey */
.tbl_vert_color_inside .bg_sum_7, .tbl_hori_color_inside .bg_sum_7 {background-color:#e1dbe5!important;} /* purple + grey */




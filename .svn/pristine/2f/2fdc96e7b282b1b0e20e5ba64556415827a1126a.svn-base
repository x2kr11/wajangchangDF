
/*==================================
Table (Horizontal) : tbl_hori
==================================*/
.tbl_hori {border-top:1px solid #4b4750;}
.tbl_hori_inside {width:100%; table-layout:fixed; border:1px solid #d5d5d5/*#ccd2de*/; border-top:none; background-color:#fff;}
.tbl_hori_inside .hori_t_row {border-bottom:1px solid #ccd2de;}
.tbl_hori_inside .hori_t_data {color:#333; padding:6px 10px 4px 10px; border-right: 1px solid #ccd2de; word-wrap:break-word;}
.tbl_hori_inside tbody .hori_t_head {color:#000; text-align:left; font-weight:normal; height:24px; /*padding:4px 10px;*/padding:4px 10px 2px 10px; border-right:1px solid #ccd2de; background-color:#eef3fd;}
.tbl_hori_inside .hori_t_data .jqTransformInputWrapper + span {margin-left:5px;}

/* 테이블 안에 테이블 */
.tbl_hori .tbl_hori,
.tbl_hori .tbl_vert {margin-top:5px; margin-bottom:5px;}


/*==================================
Table Skin (Horizontal) : tbl_hori
==================================*/
/* skin01 */
.tbl_hori.skin01 .hori_t_row,
.tbl_hori.skin01 .hori_t_data,
.tbl_hori.skin01 tbody .hori_t_head {border-color:#d5d5d5;}
.tbl_hori.skin01 tbody .hori_t_head {background-color:#eee;} 

/* skin02 */
.tbl_hori.skin02 .hori_t_row {border-top-width:0;}
.tbl_hori.skin02 .hori_t_head {border-bottom:1px solid transparent; border-color:#b2b9c7; background-color:#dbdfe3;} 
.tbl_hori.skin02 .hori_t_data {border-bottom-width:1px; background-color:#f8f8f8;}

/* skin03 */
.tbl_hori.skin03 {border-color:#727272;}
.tbl_hori.skin03 .tbl_hori_inside {border-width:0 1px 0 0; border-color:#d9d9d9;}
.tbl_hori.skin03 .hori_t_row {border-width:0;}
.tbl_hori.skin03 .hori_t_head {border:1px solid #a1a9a7; border-width:0 1px 1px 1px; background-color:#d4dbd9;} 
.tbl_hori.skin03 .hori_t_data {border-right-width:0; border-bottom:1px solid transparent; border-color:#d9d9d9;}

/* skin04 */
.tbl_hori.skin04 {border-color:#807b77;}
.tbl_hori.skin04 .tbl_hori_inside {border-width:0 1px 0 0; border-color:#bdb9b6;}
.tbl_hori.skin04 .hori_t_row {border-width:0;}
.tbl_hori.skin04 .hori_t_head {border:1px solid #bdb9b6; border-width:0 1px 1px 1px; background-color:#e6ded6;} 
.tbl_hori.skin04 .hori_t_data {border-right-width:0; border-bottom:1px solid transparent; border-color:#d9d9d9;}

/* skin05 */
.tbl_hori.skin05 .tbl_hori_inside {border-width:0 1px 0 0; border-color:#d3d2d3;}
.tbl_hori.skin05 .hori_t_row {border-width:0;}
.tbl_hori.skin05 .hori_t_head {border:1px solid #c4bfc8; border-width:0 1px 1px 1px; background-color:#ebe7ef;} 
.tbl_hori.skin05 .hori_t_data {border-right-width:0; border-bottom:1px solid transparent; border-color:#d3d2d3;}

/* skin06 */
.tbl_hori.skin06 {border-color:#777a80;}
.tbl_hori.skin06 .tbl_hori_inside {border-width:0 1px 0 0; border-color:#d3d2d3;}
.tbl_hori.skin06 .hori_t_row {border-width:0;}
.tbl_hori.skin06 .hori_t_head {border:1px solid #acb5c0; border-width:0 1px 1px 1px; background-color:#d1d8e1;} 
.tbl_hori.skin06 .hori_t_data {border-right-width:0; border-bottom:1px solid transparent; border-color:#d2d3d4;}


/*==================================
Table (Horizontal, Color) : tbl_hori_color
==================================*/
/* Basic */
.tbl_hori_color{border-top:1px solid #4b4750;}
.tbl_hori_color_inside {width:100%; text-align:center; color:#333333; background-color:#fff;}
.tbl_hori_color_inside tbody {border-top:1px solid #4b4750;}
.tbl_hori_color_inside .hori_t_data {padding:6px 10px;  border: 1px solid #ccd2de; }
.tbl_hori_color_inside thead .hori_t_head {font-weight:bold; padding:6px 10px; border: 1px solid #ccd2de;
border-collapse: collapse; background-color:#eef3fd;}
.tbl_hori_color_inside thead tr:first-child .hori_t_head {border-top:none; border-bottom:1px solid #aaa;}
.tbl_hori_color_inside .hori_t_head .array_section {position:relative;}
.tbl_hori_color_inside .hori_t_head .tit{display:inline-block; margin-right:10px;}
.tbl_hori_color_inside tbody .hori_t_head {border: 1px solid #ccd2de;}
.tbl_hori_color_inside .hori_t_head .array{ vertical-align:middle; color:#333333;}
.tbl_hori_color_inside .hori_t_head .hori_t_ico {display: inline-block; position:absolute; top:50%; right:0px; margin-top:-3px; background: url("../images/table01/tbl_hori_color.png"); vertical-align:middle;}
.tbl_hori_color_inside .hori_t_head .hori_t_ico.ico_descending {display:inline-block; background-position: left top; width: 7px; height: 4px;}
.tbl_hori_color_inside .hori_t_head .hori_t_ico.ico_ascending {display:inline-block; background-position: 30px top; width: 7px; height: 4px;}

/* Sum */
.tbl_hori_color.sum .hori_t_row.sum .hori_t_data,
.tbl_hori_color.sum .hori_t_row.sum .hori_t_head{border-bottom:1px solid #ccd2de;}
.tbl_hori_color.sum tbody .hori_t_row .hori_t_head {border-right:1px solid #ccd2de; font-weight:normal;}
.tbl_hori_color.sum .hori_t_row .hori_t_data.sum,
.tbl_hori_color.sum .hori_t_row.sum, 
.tbl_hori_color.sum .hori_t_row.sum .hori_t_head {font-weight: bold;}

/* Combine */
.tbl_hori_color.combine .hori_t_row.combine .hori_t_data,
.tbl_hori_color.combine .hori_t_row .hori_t_data.combine {border-bottom: 1px solid #ccd2de;}

/* BG Color */
.tbl_hori_color_inside .bg_1 {background-color:#fef3e2;} /* orange */
.tbl_hori_color_inside .bg_2 {background-color:#e5f3f8;} /* blue */
.tbl_hori_color_inside .bg_3 {background-color:#ffeeef;} /* pink */
.tbl_hori_color_inside .bg_4 {background-color:#f4f6e3;} /* green */
.tbl_hori_color_inside .bg_sum_2 {background-color:#e5ede8} /* blue + orange */
.tbl_hori_color_inside .bg_sum_3 {background-color:#fee7df} /* pink + orange */
.tbl_hori_color_inside .bg_5 {background-color:#edeef3} /* grey */
.tbl_hori_color_inside .bg_6 {background-color:#e2e4ee} /* dark grey */


/*==================================
Table (Vertical) : tbl_vert
==================================*/
.tbl_vert{border-top:1px solid #4b4750;/* overflow-x:auto; width:100%;*/}

/* Basic */
.tbl_vert_inside {width:100%; color:#333333; background-color:#fff; table-layout:fixed; border-collapse:collapse;}
.tbl_vert_inside thead .vert_t_head {border:1px solid #ccd2de; border-bottom-color:#ccd2de; border-top:none;}
.tbl_vert_inside tbody .vert_t_head, .tbl_vert tfoot .vert_t_head {border:1px solid #ccd2de;} 
.tbl_vert_inside .vert_t_data {padding:4px 10px 2px 10px; height:24px; border:1px solid #ccd2de; border-top-width:0; position:relative;}
.tbl_vert_inside thead .vert_t_head {font-weight:normal; padding:6px 10px 4px 10px; border-right:1px solid #ccd2de; background-color:#eef3fd; text-align:center; color:#000;}
.tbl_vert_inside tbody {border-top:1px solid #aaa;}

/* Staticcs */
.tbl_vert.statics tfoot {background-color:#eef3fd;}
.tbl_vert.statics tfoot .vert_t_head {border-right: 1px solid #ccd2de;}
.tbl_vert.statics tbody .vert_t_head {border-right:1px solid #d9d9d9; font-weight: normal;}

/* Input */
.tbl_vert_inside.input label + .jqTransformRadioWrapper {margin-left:15px;}

/* Array */
.tbl_vert_inside .tbl_array {position:relative;}
.tbl_vert_inside .tbl_array_anchor {cursor:pointer;}
.tbl_vert_inside .array_list {position:absolute; display:none; left:50%; top:23px; font-weight:normal; box-sizing:border-box; width:180px; padding:10px 15px; border-radius:5px; margin-left:-90px; background-color:rgba(59, 71, 88 ,.85);}
.tbl_vert_inside .array_list .array_box {text-align:left; }
.tbl_vert_inside .array_list .array_item {padding:2px 0;}
.tbl_vert_inside .array_list a {color:#adbed9; cursor:pointer;}
.tbl_vert_inside .array_list a:hover {color:#fff;}

/* Icon */
.tbl_vert_ico {display: inline-block; background: url(../images/table01/tbl_vert.png) no-repeat;}
.tbl_vert_ico_arrow_up {width:8px; height:6px; background-position:left top;}
.tbl_vert_ico_arrow_down {width:8px; height:6px; background-position:-50px top;}



.tbl_vert a:link, .tbl_vert a:visited, .tbl_vert a:active, .tbl_hori a:link, .tbl_hori a:visited, .tbl_hori a:active{color:#0274c4; text-decoration:none;}
.tbl_vert a:hover, .tbl_hori a:hover{text-decoration:underline;}
.tbl_vert .btn_form a:link, .tbl_vert .btn_form a:visited, .tbl_vert .btn_form a:active, .tbl_hori .btn_form a:link, .tbl_hori .btn_form a:visited, .tbl_hori .btn_form a:active{text-decoration:none!important;}
.tbl_vert .btn_form a:hover, .tbl_hori .btn_form a:hover, .tbl_vert .btn_txt:hover, .tbl_hori .btn_txt:hover{text-decoration:none!important;}



